        "targets": {
          "esmodules": true,
          // "chrome": "58",
          // "ie": "11"
        },
        "useBuiltIns": "usage",
        // "debug": false,
        "corejs": 3     // core-js 的版本
      }
    ],
    [
      "@babel/preset-react"
    ]
  ],
  "plugins": [
    ["inline-svg"],
